stage('SSH transfer') {
 script {
  sshPublisher(
   continueOnError: false, failOnError: true,
   publishers: [
    sshPublisherDesc(
     configName: "kalyan mit ",
     verbose: true,
     transfers: [
      sshTransfer(
       sourceFiles: "**/*",
       remoteDirectory: "//var//www//angular",
       execCommand: "yum install -y docker"
      )
     ])
   ])
 }
}
